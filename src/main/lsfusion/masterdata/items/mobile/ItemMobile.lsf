MODULE ItemMobile;

REQUIRE Item, BarCode;

NAMESPACE MasterData;

FORM dialogItemsMobile 'Выбор номенклатуры'
    OBJECTS i = Item
    PROPERTIES(i) READONLY name, id, idBarCode
    ORDERS name(i) 
;

DESIGN dialogItemsMobile {
    OBJECTS {
        MOVE GRID(i);
        REMOVE TOOLBARBOX(i);
    }
}
