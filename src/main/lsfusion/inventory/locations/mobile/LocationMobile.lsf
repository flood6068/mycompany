MODULE LocationMobile;

REQUIRE Location;

NAMESPACE Inventory;

FORM dialogLocationsMobile 'Выбор места хранения'
    OBJECTS l = Location
    PROPERTIES(l) READONLY name, canonicalName
    ORDERS name(l) 
;

DESIGN dialogLocationsMobile {
    OBJECTS {
        MOVE GRID(l);
        REMOVE TOOLBARBOX(l);
    }
}
